(custom-set-typed-variable (quote lockdown-command) (quote "i3lock -c 2f343f") (quote string))
(custom-set-typed-variable (quote init-xgamma) (quote ()) (quote boolean))
(custom-set-typed-variable (quote hibernate-command) (quote "systemctl hybrid-sleep") (quote string))
(custom-set-typed-variable (quote suspend-command) (quote "systemctl suspend") (quote string))
(custom-set-typed-variable (quote halt-command) (quote "systemctl shutdown") (quote string))
(custom-set-typed-variable (quote reboot-command) (quote "systemctl reboot") (quote string))
(custom-set-typed-variable (quote place-window-mode) (quote off-center) (quote symbol))
(custom-set-typed-variable (quote move-resize-raise-window) (quote t) (quote boolean))
(custom-set-typed-variable (quote workspace-names) (quote ("Main" "Utility")) (quote (list string "Workspace names")))
(custom-set-typed-variable (quote default-frame-style) (quote arc-theme) (quote frame-style))
(custom-set-keymap (quote window-keymap) (quote (keymap (tab-raise-right-window . "H-ISO_Left_Tab") (popup-window-ops-menu . "M-m") (tab-raise-left-window . "W-ISO_Left_Tab") (raise-and-pass-through-click . "Button1-Click") (move-window-interactively . "W-Button1-Move") (popup-window-ops-menu . "W-Button2-Click") (raise-lower-window . "W-Button3-Click") (lower-window . "W-Down") (raise-window . "W-Up"))))
(custom-set-keymap (quote global-keymap) (quote (keymap (cabinet-switch . "C-TAB") ((run-shell-command "rofi -combi-modi window,drun -show combi") . "Super-F2") (cycle-windows . "meta-TAB") (next-workspace . "W-Right") (previous-workspace . "W-Left"))))
(custom-set-typed-variable (quote wm-modifier-value) (quote (super)) (quote modifier-list))
(custom-set-typed-variable (quote arc-c:theme-variant) (quote Dark) (quote (choice Arc Dark)))
(custom-set-typed-variable (quote reload-themes-when-changed) (quote t) (quote boolean))
(custom-set-typed-variable (quote xterm-program) (quote "xfce4-terminal") (quote string))
(custom-set-typed-variable (quote browser-program) (quote "vivaldi") (quote string))
